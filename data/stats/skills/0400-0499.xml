	<skill id="406" levels="3" name="Angelic Icon">
		<!--
			Done by L2Dream 
			By the will of your God, your fighting ability with one-handed swords significantly increases.
			While in use, the effects of HP regeneration magic significantly decreases.
			This skill is only available when your HP is at 30 percent or lower.
			Enchant #1: Increases physical P. Def./magic P. Def/Atk. Spd.
			Enchant #2: Decreases HP consumption
		-->
		<table name="#Tab-runSpd">10 20 30</table>
		<table name="#Tab-pAtkSpd">1.1 1.2 1.3</table>
		<table name="#baseCrit">33 66 100</table>
		<table name="#cAtk">33 66 100</table>
		<table name="#Tab-Def">1.5</table>
		<table name="#Tab-hpEff">0.2</table>
		<table name="#time">60</table>
		<table name="#stackOrder">1 2 3</table>
		<table name="#reuseDelay">600000</table>
		<set name="reuseDelay" val="#reuseDelay" />
		<set name="target" val="TARGET_SELF" />
		<set name="skillType" val="BUFF" />
		<set name="operateType" val="OP_ACTIVE" />
		<cond msg="Can be only used when HP is 30% or lower">
			<player percentHP="30" />
		</cond>
		<for>
			<effect count="1" name="Buff" time="#time" stackOrder="#stackOrder" stackType="Icon" val="0">
				<mul order="0x30" stat="mDef" val="#Tab-Def" />
				<mul order="0x30" stat="pDef" val="#Tab-Def" />
				<add order="0x40" stat="accCombat" val="6" />
				<add order="0x40" stat="runSpd" val="#Tab-runSpd" />
				<mul order="0x30" stat="pAtkSpd" val="#Tab-pAtkSpd" />
				<mul order="0x30" stat="hpEff" val="#Tab-hpEff" />
				<add order="0x40" stat="baseCrit" val="#baseCrit">
					<and>
						<using kind="Sword, Big Sword, Pole" />
					</and>
				</add>
				<add order="0x40" stat="cAtk" val="#cAtk">
					<and>
						<using kind="Blunt, Big Blunt, Fist, Dual Fist" />
					</and>
				</add>
			</effect>
		</for>
	</skill>
